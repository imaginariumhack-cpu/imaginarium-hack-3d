{
  "version": 2,
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@3.0.0"
    }
  },
  "routes": [
    {
      "src": "/api-docs(.*)",
      "dest": "/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "ENABLE_SWAGGER": "true"
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "ENABLE_SWAGGER": "true"
    }
  },
  "buildCommand": "npm run build"
}